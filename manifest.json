{
  "manifest_version": 3,
  "name": "ImgScraper",
  "description": "웹 페이지의 캡처와 함께 캡처한 사이트의 URL을 따로 저장하는 웹앱 입니다.",
  "version": "1.0",
  "action": {
    "default_popup" : "popup/index.html",
    "default_icon": {               
      "16": "images/icon_16.png",
      "128": "images/icon_128.png"
    },
    "default_title" : "ImgScraper"
  },
  "background": {
    "service_worker" : "background.js"
  }
}